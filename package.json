{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Starting boilerplate for most of my repos. To duplicate the repo: https://help.github.com/articles/duplicating-a-repository/ Use 'npm update --save' and 'npm update --save-dev' to update the versions and after a while use 'npm prune' to remove unused dependencies. Make sure all dependency version numbers are * before updating.",
  "main": "start.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack --config webpack.config.development.js",
    "build:umd:min": "webpack --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf lib dist",
    "prepublish": "npm run clean && npm run build",
    "server": "better-npm-run server",
    "server:dev": "better-npm-run server:dev",
    "start": "npm run build && npm run server",
    "start:dev": "npm run server:dev"
  },
  "betterScripts": {
    "server": {
      "command": "node start.js",
      "env": {
        "NODE_ENV": "production",
        "MIN_EXT": ".min"
      }
    },
    "server:dev": {
      "command": "node start.js",
      "env": {
        "NODE_ENV": "development",
        "MIN_EXT": ""
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kilkelly/scaffold.git"
  },
  "author": "Frank Kilkelly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kilkelly/scaffold/issues"
  },
  "homepage": "https://github.com/kilkelly/scaffold#readme",
  "dependencies": {
    "axios": "*",
    "detect-node": "*",
    "history": "*",
    "immutable": "*",
    "moment": "*",
    "react": "*",
    "react-addons-pure-render-mixin": "*",
    "react-dom": "*",
    "react-redux": "*",
    "react-router": "*",
    "redux": "*",
    "redux-immutable": "*",
    "redux-thunk": "*",
    "validator": "*"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-plugin-react-transform": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-react-hmre": "*",
    "babel-register": "*",
    "better-npm-run": "*",
    "css-loader": "*",
    "express": "*",
    "node-sass": "*",
    "redux-devtools": "*",
    "redux-devtools-dock-monitor": "*",
    "redux-devtools-log-monitor": "*",
    "redux-logger": "*",
    "rimraf": "*",
    "sass-loader": "*",
    "style-loader": "*",
    "webpack": "*",
    "webpack-dev-middleware": "*",
    "webpack-hot-middleware": "*"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
